<h1>People#index</h1>
<p>Find me in app/views/people/index.html.erb</p>


<table class="table">
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Age</th>
    <th>Mail</th>
  </tr>
    <% @dates.each do |data| %>
  <tr>
    <td><%= data.id %></td>
    <td><%= data.name %></td>
    <td><%= data.age %></td>
    <td><%= data.mail %></td>
  </tr>
  <% end %>
</table>